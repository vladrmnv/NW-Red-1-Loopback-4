{
  "name": "@nestwealth/deaccumulation-demo",
  "version": "1.0.0",
  "description": "NW Deacc demo",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "ts-node-dev index.ts",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "ts-node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^0.14.5",
    "@loopback/context": "^0.13.1",
    "@loopback/core": "^0.11.16",
    "@loopback/dist-util": "^0.3.7",
    "@loopback/openapi-v3": "^0.15.4",
    "@loopback/repository": "^0.21.0",
    "@loopback/rest": "^0.25.4",
    "@loopback/service-proxy": "^0.8.10",
    "tsconfig-paths": "^3.6.0"
  },
  "devDependencies": {
    "@loopback/build": "^0.7.6",
    "@loopback/testlab": "^0.15.1",
    "@types/node": "^10.11.2"
  }
}
